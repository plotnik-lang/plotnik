---
source: crates/plotnik-lib/src/emit/codegen_tests.rs
---
Expr = [
  Lit: (number) @value :: string
  Nested: (call_expression function: (identifier) @fn arguments: (Expr) @inner)
]

Test = (program (Expr) @expr)
---
[flags]
linked = false

[strings]
S00 "Beauty will save the world"
S01 "value"
S02 "fn"
S03 "inner"
S04 "Lit"
S05 "Nested"
S06 "expr"
S07 "Expr"
S08 "Test"
S09 "number"
S10 "call_expression"
S11 "arguments"
S12 "function"
S13 "identifier"
S14 "program"

[type_defs]
T00 = <Node>
T01 = <String>
T02 = Struct  M0:1  ; { value }
T03 = Struct  M1:2  ; { fn, inner }
T04 = Enum    M3:2  ; Lit | Nested
T05 = Struct  M5:1  ; { expr }
T06 = Struct  M6:1  ; { fn }
T07 = Struct  M7:1  ; { inner }

[type_members]
M0: S01 → T01  ; value: <String>
M1: S02 → T00  ; fn: <Node>
M2: S03 → T04  ; inner: Expr
M3: S04 → T02  ; Lit: T02
M4: S05 → T03  ; Nested: T03
M5: S06 → T04  ; expr: Expr
M6: S02 → T00  ; fn: <Node>
M7: S03 → T04  ; inner: Expr

[type_names]
N0: S07 → T04  ; Expr
N1: S08 → T05  ; Test

[entrypoints]
Expr = 06 :: T04
Test = 09 :: T05

[transitions]
_ObjWrap:
  00   ε   [Obj]                            02
  02       Trampoline                       03
  03   ε   [EndObj]                         05
  05                                        ▶

Expr:
  06   ε                                    07
  07   ε                                    20, 26

Test:
  09   ε                                    10
  10   !   (program)                        11
  11   ▽   (Expr)                           06 : 12
  12   ε   [Set(M5)]                        14
  14   △                                    15
  15                                        ▶
  16   ε   [Set(M2)]                        18
  18   △                                    19
  19                                        ▶
  20   !   [Enum(M3)] (number) [Text Set(M0) EndEnum]  19
  23   ▷   arguments: (Expr)                06 : 16
  24   ▽   function: (identifier) [Node Set(M1)]  23
  26   !   [Enum(M4)] (call_expression) [EndEnum]  24
